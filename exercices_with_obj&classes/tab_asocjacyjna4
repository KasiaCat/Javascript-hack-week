var guests = [];
guests["Kowalski"] = 1;
guests["Nowak"] = 2;
guests["Bravo"] = 8;
guests["CIA"] = 9;
guests["Nowakowska"] = 10;

for (var nazwisko  in guests) {
        if (nazwisko == "CIA")
        {
            continue;
        }
        console.log(nazwisko + " w pokoju " + guests[nazwisko])
}